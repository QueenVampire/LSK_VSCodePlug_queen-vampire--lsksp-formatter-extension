{
  "name": "queen-vampire--lsksp-formatter-extension",
  "displayName": "queen-vampire--lsksp-formatter-extension",
  "description": "queen-vampire--lsksp-formatter-extension",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./extension.js",
  "repository":{},
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "languages": [{
      "id": "lsksp",
      "aliases": ["lsksp", "lsksp"],
      "extensions": ["lsksp"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "lsksp",
      "scopeName": "source.lsksp",
      "path": "./syntaxes/lsksp.tmLanguage.json"
    }]
  }
}
